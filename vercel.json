{
    "rewrites": [{ "source": "/api/(.*)", "destination": "/api/$1" }],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                { "key": "X-Robots-Tag", "value": "noindex" },
                { "key": "Permissions-Policy", "value": "interest-cohort=()" }
            ]
        }
    ]
}
